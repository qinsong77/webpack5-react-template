{
  "name": "webpack5-react-template",
  "private": true,
  "version": "1.0.0",
  "description": "webpack5 react base template",
  "scripts": {
    "prepare": "husky & npx only-allow pnpm",
    "commit": "git-cz",
    "dev": "cross-env NODE_ENV=development DEBUG=true TS_NODE_PROJECT=\\\"tsconfig.webpack.json\\\" webpack serve --config webpack/webpack.development.ts",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=\\\"tsconfig.webpack.json\\\" webpack --config webpack/webpack.production.ts",
    "build:analyzer": "cross-env NODE_ENV=production analyzer=true TS_NODE_PROJECT=\\\"tsconfig.webpack.json\\\" webpack --config webpack/webpack.production.ts",
    "preview": "serve dist -p 3001",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .js,.ts,.tsx ./src ./webpack --ignore-path .gitignore",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx ./src ./webpack --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "pnpm run prettier -- --write",
    "check-types": "tsc --project tsconfig.json --diagnostics --pretty --noEmit && echo \"Types check passed\"",
    "check-format": "npm run prettier -- --list-different",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:chromium": "playwright test --project=chromium",
    "codegen:api": "npx ts-codegen && prettier --write src/service/apis/*.ts"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "keywords": [
    "react",
    "webpack5",
    "modern web app"
  ],
  "author": "sysuke",
  "license": "ISC",
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@tanstack/react-query": "^5.32.0",
    "axios": "^1.7.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.1.7",
    "lucide-react": "^0.436.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@commitlint/cli": "^19.3.0",

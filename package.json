{
  "name": "webpack5-react-template",
  "private": true,
  "version": "1.0.0",
  "description": "webpack5 react base template",
  "scripts": {
    "prepare": "husky install & npx only-allow pnpm",
    "commit": "git-cz",
    "dev": "cross-env NODE_ENV=development DEBUG=true TS_NODE_PROJECT=\\\"tsconfig.webpack.json\\\" webpack serve --config webpack/webpack.development.ts",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=\\\"tsconfig.webpack.json\\\" webpack --config webpack/webpack.production.ts",
    "build:analyzer": "cross-env NODE_ENV=production analyzer=true TS_NODE_PROJECT=\\\"tsconfig.webpack.json\\\" webpack --config webpack/webpack.production.ts",
    "preview": "serve dist -p 3001",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .js,.ts,.tsx ./src ./webpack --ignore-path .gitignore",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx ./src ./webpack --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "pnpm run prettier -- --write",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit && echo \"Types check passed\"",
    "check-format": "npm run prettier -- --list-different",
    "test": "jest",
    "codegen:api": "npx ts-codegen && prettier --write src/service/apis/*.ts"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "keywords": [
    "react",
    "webpack5",
    "modern web app"
  ],
  "author": "sysuke",
  "license": "ISC",
  "dependencies": {
    "@tanstack/react-query": "^5.17.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "axios": "^1.6.0",
    "clsx": "^2.1.0",
    "framer-motion": "^10.16.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.2",
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "@tanstack/react-query-devtools": "^5.17.0",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@ts-tool/ts-codegen-cli": "^3.3.4",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.6.1",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "commitizen": "^4.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.35.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.9.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",

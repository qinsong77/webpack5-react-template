{
  "name": "webpack5-react-template",
  "private": true,
  "version": "1.0.0",
  "description": "webpack5 react base template",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install & npx only-allow pnpm",
    "commit": "git-cz",
    "dev": "cross-env NODE_ENV=development TS_NODE_PROJECT=\\\"tsconfig.webpack.json\\\" webpack serve --config webpack/webpack.development.ts",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=\\\"tsconfig.webpack.json\\\" webpack --config webpack/webpack.production.ts",
    "build:analyzer": "cross-env NODE_ENV=production analyzer=true TS_NODE_PROJECT=\\\"tsconfig.webpack.json\\\" webpack --config webpack/webpack.production.ts",
    "serve": "serve dist -p 3001",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .js,.ts,.tsx ./src ./webpack --ignore-path .gitignore",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx ./src ./webpack --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "pnpm run prettier -- --write",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "check-format": "npm run prettier -- --list-different",
    "test": "jest"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.2.6",
    "@babel/preset-typescript": "^7.21.0",
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.6.1",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "commitizen": "^4.3.0",
    "core-js": "^3.29.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-testing-library": "^5.10.2",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "mini-css-extract-plugin": "^2.7.3",
    "postcss": "^8.4.14",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.3.2",
    "postcss-nested": "^6.0.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^8.4.1",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "react-refresh": "^0.14.0",
    "serve": "^14.2.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.3.2",
    "terser-webpack-plugin": "^5.3.6",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.82.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.14.0",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  }
}

{
  "name": "webpack5-react-template",
  "private": true,
  "version": "1.0.0",
  "description": "webpack5 react bse template",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install & npx only-allow pnpm",
    "dev": "cross-env NODE_ENV=development TS_NODE_PROJECT=\\\"tsconfig-for-webpack-config.json\\\" webpack serve --config webpack/webpack.development.ts",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=\\\"tsconfig-for-webpack-config.json\\\" webpack --config webpack/webpack.production.ts",
    "serve": "serve dist -p 3001",
    "clean": "rm -rf dist",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\"  --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint  --fix"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",

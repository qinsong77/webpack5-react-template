    "composite": true,
    "module": "esnext",
    "target": "ESNext",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true
  },
  "include": ["webpack"],
  "exclude": ["node_modules"]
}
